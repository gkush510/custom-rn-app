/* Main Classes */
.myinput[type="checkbox"]:before{
  position: relative;
  display: block;
  width: 11px;
  height: 11px;
  border: 0;
  content: "";
  background:transparent;
}
.myinput[type="checkbox"]:after{
  position: relative;
  display: block;
  left: 2px;
  top: -11px;
  width: 7px;
  height: 7px;
  border-width: 0;
  border-style: solid;
  border-color: transparent;
  content: "";
 
  background-repeat: no-repeat;
  background-position:center;
}
